package kr.or.ddit.servlet03;

import javax.servlet.http.HttpServlet;

public class CalculateServlet extends HttpServlet{
	enum operator(String) {
		PLUS("+"),
		MINUS("-"),
		MULTIPLY("*"),
		DIVIDE("/");

	}
}










/*
PLUS">+</option>
		<option value="MINUS">-</option>
		<option value="MULTIPLY">*</option>
		<option value="DIVIDE"



*/
